    "@angular/common": "2.2.0-beta.0",
    "@angular/compiler": "2.2.0-beta.0",
    "@angular/core": "2.2.0-beta.0",
    "@angular/forms": "2.2.0-beta.0",
    "@angular/http": "2.2.0-beta.0",
    "@angular/platform-browser": "2.2.0-beta.0",
    "@angular/platform-browser-dynamic": "2.2.0-beta.0",
    "@angular/router": "3.2.0-beta.0",
    "@angular/upgrade": "2.2.0-beta.0",
    "@ngrx/core": "1.2.0",
